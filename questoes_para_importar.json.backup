  { "texto_enunciado": "O Estatuto da Criança e do Adolescente (ECA) define como crime a conduta de vender, fornecer ou entregar à criança ou ao adolescente, sem justa causa, produtos cujos componentes possam causar dependência física ou psíquica.", "alternativas": [ { "letra": "A", "texto": "Certo." }, { "letra": "B", "texto": "Errado, pois é apenas infração administrativa." }, { "letra": "C", "texto": "Certo, mas apenas para adolescentes." }, { "letra": "D", "texto": "Errado, pois depende da quantidade vendida." }, { "letra": "E", "texto": "Certo, mas apenas se o produto for ilícito." } ], "resposta_correta": "A", "banca": "FGV", "concurso": "Exame de Ordem Unificado (OAB)", "ano": 2025 },
  { "texto_enunciado": "O advogado substabelecido com reserva de poderes:", "alternativas": [ { "letra": "A", "texto": "não pode praticar atos no processo." }, { "letra": "B", "texto": "passa a ser o único advogado na causa." }, { "letra": "C", "texto": "atua em conjunto com o advogado substabelecente, que permanece com seus poderes no processo." }, { "letra": "D", "texto": "precisa de uma nova procuração do cliente." }, { "letra": "E", "texto": "só pode atuar em audiências." } ], "resposta_correta": "C", "banca": "FGV", "concurso": "Exame de Ordem Unificado (OAB)", "ano": 2025 },
  { "texto_enunciado": "A sociedade de advogados:", "alternativas": [ { "letra": "A", "texto": "pode ter caráter mercantil e adotar nome fantasia." }, { "letra": "B", "texto": "não pode ser uma sociedade unipessoal." }, { "letra": "C", "texto": "deve ser registrada exclusivamente na Junta Comercial." }, { "letra": "D", "texto": "adquire personalidade jurídica com o registro de seus atos constitutivos no Conselho Seccional da OAB em cuja base territorial tiver sede." }, { "letra": "E", "texto": "pode ser composta por sócios não advogados." } ], "resposta_correta": "D", "banca": "FGV", "concurso": "Exame de Ordem Unificado (OAB)", "ano": 2025 },
  { "texto_enunciado": "A respeito dos honorários advocatícios, os honorários sucumbenciais são:", "alternativas": [ { "letra": "A", "texto": "pagos pelo cliente ao seu próprio advogado." }, { "letra": "B", "texto": "fixados em contrato." }, { "letra": "C", "texto": "pagos pela parte vencida no processo ao advogado da parte vencedora." }, { "letra": "D", "texto": "devidos quando há um acordo entre as partes." }, { "letra": "E", "texto": "sempre um percentual sobre o valor da causa." } ], "resposta_correta": "C", "banca": "FGV", "concurso": "Exame de Ordem Unificado (OAB)", "ano": 2025 },
  { "texto_enunciado": "Constitui infração disciplinar, punível com censura, o advogado que:", "alternativas": [ { "letra": "A", "texto": "violar, sem justa causa, sigilo profissional." }, { "letra": "B", "texto": "abandonar a causa sem justo motivo e prévia ciência do cliente." }, { "letra": "C", "texto": "apropriar-se de valores do cliente." }, { "letra": "D", "texto": "fizer prova falsa." }, { "letra": "E", "texto": "praticar crime infamante." } ], "resposta_correta": "A", "banca": "FGV", "concurso": "Exame de Ordem Unificado (OAB)", "ano": 2025 },
  { "texto_enunciado": "A Ação Declaratória de Constitucionalidade (ADC) tem como um de seus objetivos:", "alternativas": [ { "letra": "A", "texto": "retirar uma lei do ordenamento jurídico." }, { "letra": "B", "texto": "confirmar a constitucionalidade de lei ou ato normativo federal para sanar controvérsia judicial relevante." }, { "letra": "C", "texto": "permitir que o cidadão questione uma lei municipal." }, { "letra": "D", "texto": "suprir a omissão do legislador." }, { "letra": "E", "texto": "declarar a constitucionalidade de uma lei estadual." } ], "resposta_correta": "B", "banca": "FGV", "concurso": "Exame de Ordem Unificado (OAB)", "ano": 2025 },
  { "texto_enunciado": "Em um contrato de compra e venda, o vício redibitório é um:", "alternativas": [ { "letra": "A", "texto": "defeito aparente e de fácil constatação." }, { "letra": "B", "texto": "defeito oculto que torna a coisa imprópria ao uso a que se destina, ou lhe diminua o valor." }, { "letra": "C", "texto": "erro sobre a identidade do objeto." }, { "letra": "D", "texto": "problema na documentação do bem." }, { "letra": "E", "texto": "atraso na entrega do bem." } ], "resposta_correta": "B", "banca": "FGV", "concurso": "Exame de Ordem Unificado (OAB)", "ano": 2025 },
