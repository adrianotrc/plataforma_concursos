  { "texto_enunciado": "O lançamento do Imposto sobre a Propriedade de Veículos Automotores (IPVA) é, tipicamente, um lançamento:", "alternativas": [ { "letra": "A", "texto": "por homologação." }, { "letra": "B", "texto": "por declaração." }, { "letra": "C", "texto": "de ofício." }, { "letra": "D", "texto": "por arbitramento." }, { "letra": "E", "texto": "misto." } ], "resposta_correta": "C", "banca": "Fundação Carlos Chagas (FCC)", "concurso": "SEFAZ-SP - Agente Fiscal de Rendas", "ano": 2013 },
  { "texto_enunciado": "De acordo com o CTN, a obrigação tributária principal surge com a ocorrência do:", "alternativas": [ { "letra": "A", "texto": "lançamento." }, { "letra": "B", "texto": "pagamento." }, { "letra": "C", "texto": "fato gerador." }, { "letra": "D", "texto": "inscrição em dívida ativa." }, { "letra": "E", "texto": "auto de infração." } ], "resposta_correta": "C", "banca": "Fundação Carlos Chagas (FCC)", "concurso": "SEFAZ-SP - Agente Fiscal de Rendas", "ano": 2013 },
